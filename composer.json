{
    "name": "miisieq/robots-txt-bundle",
    "type": "symfony-bundle",
    "description": "Robots.txt generator for Symfony 2, 3 & 4",
    "keywords": [
        "robots",
        "robots.txt",
        "bundle",
        "symfony"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Miisieq\\RobotsTxtBundle\\": ""
        }
    },
    "require": {
        "php": "^7.1",
        "symfony/config": "~2.0|~3.0|~4.0",
        "symfony/http-kernel": "~2.0|~3.0|~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7 | ^6 | ^7",
        "symfony/yaml": "~2.0|~3.0|~4.0",
        "symfony/dependency-injection": "~2.0|~3.0|~4.0"
    }
}
